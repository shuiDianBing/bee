<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.stynet.bee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_bee_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".viewLayer.LauncherActivity" android:theme="@style/launcherStyle">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".viewLayer.activity.SigninActivity" android:theme="@style/launcherStyle"/>
        <activity android:name=".viewLayer.activity.HomeActivity"/>
        <activity android:name="com.stynet.widget.activity.BubbleApiActivity"/>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WAKE_LOCK"/>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>MissingPermission
</manifest>